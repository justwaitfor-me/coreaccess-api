{
    "name": "vendor/php-auth-app",
    "description": "A scalable PHP authentication application with API versioning.",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "firebase/php-jwt": "^5.0",
        "vlucas/phpdotenv": "^5.0",
        "monolog/monolog": "^2.0",
        "slim/slim": "^4.0",
        "phpunit/phpunit": "^9.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/process": "^5.0"

    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}